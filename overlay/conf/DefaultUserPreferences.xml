<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<!DOCTYPE preferences PUBLIC "yes" "http://java.sun.com/dtd/preferences.dtd">
<preferences EXTERNAL_XML_VERSION="1.0">
    <root type="user">
        <map/>
        <node name="rx">
            <map/>
            <node name="user">
                <map>
                    <entry key="lastOpenProject" value=""/>
                    <entry key="CoordinateSystem" value="Decimal Degrees"/>
                    <entry key="SecurityFilter" value="High"/>
                    <entry key="DateFormat" value="yyyy-MM-dd HH:mm:ss z"/>
                    <entry key="TimeZone" value="UTC"/>
                    <entry key="GlobalIconScale" value="1.0"/>
                    <entry key="RestoreLastView" value="true"/>
                    <entry key="AudioAlerts" value="Alerts On"/>
                    <!--<entry key="lastDisplayUoM" value="KM/KMÂ²"/>-->

                    <!-- This value represents whether to draw a box around the bounds of an imagery layer if it is visible.
                    It will be drawn in yellow. The default value is false-->
                    <entry key="drawImageryLayerBounds" value="false"/>

                    <!-- This value represents whether to draw a box around the bounds of an elevation layer if it is visible.
                    It will be drawn in green. The default value is false-->
                    <entry key="drawElevationLayerBounds" value="false"/>

                    <!-- Specify the default skin for the user. Requires knowledge of the skin's class name. This value
                         will be updated if the user changes skins via Raptor's preference options.-->
                    <entry key="skin" value="gov.raptor.core.view.gui.skins.DefaultRaptorSkin"/>
                    <!-- entry key="skin" value="org.pushingpixels.substance.api.skin.BusinessBlackSteelSkin"/ -->

                    <!-- Specify whether user drawn shapes should be labeled by default -->
                    <entry key="showShapeLabels" value="false"/>

                    <!-- Specifies the level of track detail (number of tracks to show) before grouping logic is
                              applied. The definition of the allowable levels (low/medium/high) are defined in the TrackDetailLevels
                              node of the ProgramProperties file. -->
                    <entry key="TrackDetailLevel" value="high"/>

                    <!-- Prompt the user when they invoke a quitting action to confirm that they do in fact want to
                         do this -->
                    <entry key="ConfirmQuit" value="true"/>
                </map>
                <node name="RibbonBarConfiguration">
                    <map>
                        <entry key="DisplayDefaultConfiguration" value="false"/>
                        <entry key="ActiveConfiguration" value="Moving Base RibbonBar"/>
                    </map>
                </node>
                <node name="CommPorts">
                    <map/>
                    <node name="Serial">
                        <map/>
                    </node>
                </node>
                <node name="Status">
                    <map/>
                    <node name="Inactive">
                        <map>
                            <entry key="Shape" value="circle"/>
                            <entry key="Color" value="none"/>
                            <entry key="Action" value="none"/>
                        </map>
                    </node>
                    <node name="Active">
                        <map>
                            <entry key="Shape" value="circle"/>
                            <entry key="Color" value="green"/>
                            <entry key="Action" value="none"/>
                        </map>
                    </node>
                    <node name="Critical">
                        <map>
                            <entry key="Shape" value="octagon"/>
                            <entry key="Color" value="red"/>
                            <entry key="Action" value="pulsing"/>
                        </map>
                    </node>
                    <node name="Unknown">
                        <map>
                            <entry key="Shape" value="none"/>
                            <entry key="Color" value="none"/>
                            <entry key="Action" value="none"/>
                        </map>
                    </node>
                    <node name="Warning">
                        <map>
                            <entry key="Shape" value="triangle"/>
                            <entry key="Color" value="yellow"/>
                            <entry key="Action" value="none"/>
                        </map>
                    </node>
                    <node name="Serious">
                        <map>
                            <entry key="Shape" value="triangle"/>
                            <entry key="Color" value="yellow"/>
                            <entry key="Action" value="pulsing"/>
                        </map>
                    </node>
                </node>
                <node name="EventLog">
                    <map/>
                    <node name="Map Object Containment">
                        <map>
                            <entry key="Color" value="none"/>
                        </map>
                    </node>
                    <node name="Map Object Suppressed">
                        <map>
                            <entry key="Color" value="gray"/>
                        </map>
                    </node>
                    <node name="Map Object Added">
                        <map>
                            <entry key="Color" value="none"/>
                        </map>
                    </node>
                    <node name="User Entry">
                        <map>
                            <entry key="Color" value="pink"/>
                        </map>
                    </node>
                    <node name="Plugin Event">
                        <map>
                            <entry key="Color" value="cyan"/>
                        </map>
                    </node>
                    <node name="App Event">
                        <map>
                            <entry key="Color" value="cyan"/>
                        </map>
                    </node>
                </node>
                <node name="wcs">
                    <map>
                        <entry key="ImageryWMSServers" value=""/>
                        <entry key="ElevationWMSServers" value="https://geoint.nrlssc.org/nrltileserver/wcs/blorigin?"/>
                    </map>
                </node>
                <node name="wms">
                    <map>
                        <entry key="ImageryWMSServers" value="https://neowms.sci.gsfc.nasa.gov/wms/wms,https://data.worldwind.arc.nasa.gov/wms?,https://worldwind27.arc.nasa.gov/wms/virtualearth?"/>
                        <entry key="ElevationWMSServers" value="https://worldwind26.arc.nasa.gov/elev?"/>
                    </map>
                </node>
                <node name="fileStores">
                    <map/>
                </node>
            </node>
        </node>
    </root>
</preferences>
